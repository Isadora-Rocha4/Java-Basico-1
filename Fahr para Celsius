	public static void main(String[] args) {
		// TODO Auto-generated method stub
//		questao7 Fahrenheit para Celsius
		Scanner in = new Scanner(System.in);
		float f;
		System.out.println ("Digite a tempertura em F°: ");
		 f = in.nextFloat();
		 
		 System.out.println("A temperatura em C°: " +((5.0 * (f - 32.0)/9.0)));
		
	}

}
