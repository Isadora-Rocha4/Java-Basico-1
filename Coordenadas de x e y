public class questao51 {
// as coordenadas x e y de pontos no R2 e calcule sua distancia da origem(0, 0).
	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		double x, y, distancia;
		System.out.println("Digite as coordenadas x e y, respectivamente: ");
		x = in.nextDouble();
		y = in.nextDouble();
		distancia = Math.sqrt((Math.pow(x,2+Math.pow(y,2))));
		System.out.println("O valor da origem Ã©: "+distancia);

    }
}
